{
	"name": "cheers-backend",
	"version": "1.0.0",
	"description": "backend for cheers frontend",
	"main": "app.js",
	"engines": {
		"node": "8.12.0"
	},
	"scripts": {
		"postinstall": "npm --prefix client install",
		"build": "npm --prefix client run build",
		"start": "node app.js",
		"server": "nodemon app",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\""
	},
	"author": "Baby Hanuman",
	"license": "ISC",
	"dependencies": {
		"colors": "^1.4.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"mongoose": "^5.9.7",
		"morgan": "^1.10.0"
	},
	"devDependencies": {
		"concurrently": "^5.1.0",
		"nodemon": "^2.0.2"
	}
}
